                    <Font name="System Bold" size="16.0" />
                 </font>
              </Label>
              <Button fx:id="btnIniciar" layoutX="10.0" layoutY="32.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="62.0" text="Iniciar" />
              <Button fx:id="btnProxFase" layoutX="76.0" layoutY="32.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="101.0" text="Próxima fase" />
              <Button fx:id="btnFinalizar" layoutX="180.0" layoutY="32.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="62.0" text="Finalizar" />
