<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="paneDetAt" maxWidth="650.0" prefHeight="200.0" prefWidth="650.0" style="-fx-border-color: #c1c1c1; -fx-border-width: 0 0 1 0;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
     <children>
      <TableView fx:id="tblFases" layoutX="4.0" layoutY="99.0" prefHeight="97.0" prefWidth="642.0">
        <columns>
          <TableColumn fx:id="colEtapa" prefWidth="91.0" text="Etapa" />
          <TableColumn fx:id="colIniciada" prefWidth="104.0" text="Iniciada" />
            <TableColumn fx:id="colConcluida" prefWidth="93.0" text="Concluída" />
        </columns>
      </TableView>
        <Label fx:id="lblMarcaModelo" layoutX="7.0" layoutY="7.0" text="Marca / Modelo ">
           <font>
              <Font size="22.0" />
           </font>
        </Label>
        <Label fx:id="lblCor" layoutX="7.0" layoutY="60.0" text="Cor do veículo" />
      <Label fx:id="lblPlaca" layoutX="7.0" layoutY="38.0" text="ABC-0000" textFill="#649051">
         <font>
            <Font name="Lucida Console" size="20.0" />
         </font>
      </Label>
      <Pane fx:id="paneControles" layoutX="392.0" layoutY="6.0" prefHeight="85.0" prefWidth="252.0">
